{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": [
    "docker-compose"
  ],
  "dependencyDashboard": true,
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "baseBranchPatterns": [
    "main"
  ],
  "enabled": true,
  "extends": [
    "config:recommended",
    "docker:pinDigests"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "immich-app/postgres/"
      ],
      "enabled": false
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "postgres",
        "mysql",
        "mariadb"
      ],
      "enabled": false
    }
  ]
}
